@article{quillardTLR2NeutrophilsPotentiate2015,
  title = {{{TLR2}} and Neutrophils Potentiate Endothelial Stress, Apoptosis and Detachment: Implications for Superficial Erosion},
  shorttitle = {{{TLR2}} and Neutrophils Potentiate Endothelial Stress, Apoptosis and Detachment},
  author = {Quillard, Thibaut and Ara{\'u}jo, Haniel Alves and Franck, Gregory and Shvartz, Eugenia and Sukhova, Galina and Libby, Peter},
  year = {2015},
  month = jun,
  journal = {European Heart Journal},
  volume = {36},
  number = {22},
  pages = {1394--1404},
  issn = {1522-9645},
  doi = {10.1093/eurheartj/ehv044},
  abstract = {AIMS: Superficial erosion of atheromata causes many acute coronary syndromes, but arises from unknown mechanisms. This study tested the hypothesis that Toll-like receptor-2 (TLR2) activation contributes to endothelial apoptosis and denudation and thus contributes to the pathogenesis of superficial erosion. METHODS AND RESULTS: Toll-like receptor-2 and neutrophils localized at sites of superficially eroded human plaques. In vitro, TLR2 ligands (including hyaluronan, a matrix macromolecule abundant in eroded lesions) induced endothelial stress, characterized by reactive oxygen species production, endoplasmic reticulum (ER) stress, and apoptosis. Co-incubation of neutrophils with endothelial cells (ECs) potentiated these effects and induced EC apoptosis and detachment. We then categorized human atherosclerotic plaques (n = 56) based on morphologic features associated with superficial erosion, 'stable' fibrotic, or 'vulnerable' lesions. Morphometric analyses of the human atheromata localized neutrophils and neutrophil extracellular traps (NETs) near clusters of apoptotic ECs in smooth muscle cell (SMC)-rich plaques. The number of luminal apoptotic ECs correlated with neutrophil accumulation, amount of NETs, and TLR2 staining in SMC-rich plaques, but not in 'vulnerable' atheromata. CONCLUSION: These in vitro observations and analyses of human plaques indicate that TLR2 stimulation followed by neutrophil participation may render smooth muscle cell-rich plaques susceptible to superficial erosion and thrombotic complications by inducing ER stress, apoptosis, and favouring detachment of EC.},
  langid = {english},
  pmcid = {PMC4458287},
  pmid = {25755115},
  keywords = {Acute Coronary Syndrome,Apoptosis,Biomarkers,Caspases,Cell Adhesion,Cell Adhesion Molecules,Cytokines,Endothelial cells,Fluoresceins,Fluorescent Dyes,Humans,Hyaluronan,Hyaluronic Acid,Leukocyte Elastase,Leukocytes Mononuclear,Muscle Smooth Vascular,NET,Neutrophil,Neutrophils,Plaque Atherosclerotic,Protein Phosphatase 1,Reactive Oxygen Species,RNA Small Interfering,Rupture Spontaneous,Stress Physiological,Superficial erosion,TLR2,Toll-Like Receptor 2,Transcription Factor CHOP},
