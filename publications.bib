@article{thijssenDifferentialDiagnosticPerformance2022,
  title = {Differential Diagnostic Performance of a Panel of Plasma Biomarkers for Different Types of Dementia},
  author = {Thijssen, Elisabeth H. and Verberk, Inge M.W. and Kindermans, Jana and Abramian, Adlin and Vanbrabant, Jeroen and Ball, Andrew J. and Pijnenburg, Yolande and Lemstra, Afina W. and {van der Flier}, Wiesje M. and Stoops, Erik and Hirtz, Christophe and Teunissen, Charlotte E.},
  year = {2022},
  month = jan,
  journal = {Alzheimer's \& Dementia: Diagnosis, Assessment \& Disease Monitoring},
  volume = {14},
  number = {1},
  pages = {e12285},
  publisher = {John Wiley \& Sons, Ltd},
  issn = {2352-8729},
  doi = {10.1002/dad2.12285},
  urldate = {2025-08-27},
  abstract = {Abstract Introduction We explored what combination of blood-based biomarkers (amyloid beta [A?]1-42/1-40, phosphorylated tau [p-tau]181, neurofilament light [NfL], glial fibrillary acidic protein [GFAP]) differentiates Alzheimer's disease (AD) dementia, frontotemporal dementia (FTD), and dementia with Lewy bodies (DLB). Methods We measured the biomarkers with Simoa in two separate cohorts (n~=~160 and n~=~152). In one cohort, A?1-42/1-40 was also measured with mass spectrometry (MS). We assessed the differential diagnostic value of the markers, by logistic regression with Wald's backward selection. Results MS and Simoa A?1-42/1-40 similarly differentiated AD from controls. The Simoa panel that optimally differentiated AD from FTD consisted of NfL and p-tau181 (area under the curve [AUC]~=~0.94; cohort 1) or NfL, GFAP, and p-tau181 (AUC~=~0.90; cohort 2). For AD from DLB, the panel consisted of NfL, p-tau181, and GFAP (AUC~=~0.88; cohort 1), and only p-tau181 (AUC~=~0.81; cohort 2). Discussion A combination of plasma p-tau181, NfL, and GFAP, but not A?1-42/1-40, might be useful to discriminate AD, FTD, and DLB.},
  keywords = {amyloid beta,blood biomarker,glial fibrillary acidic protein,neurofilament light,phosphorylated tau},
  file = {/Users/loverma2/Zotero/storage/M3CJJMKE/Thijssen e.a. - 2022 - Differential diagnostic performance of a panel of plasma biomarkers for different types of dementia.pdf}
}

@article{tianEffectsGenderGene2012,
  title = {Effects of Gender on Gene Expression in the Blood of Ischemic Stroke Patients},
  author = {Tian, Yingfang and Stamova, Boryana and Jickling, Glen C. and Liu, Dazhi and Ander, Bradley P. and Bushnell, Cheryl and Zhan, Xinhua and Davis, Ryan R. and Verro, Piero and Pevec, William C. and Hedayati, Nasim and Dawson, David L. and Khoury, Jane and Jauch, Edward C. and Pancioli, Arthur and Broderick, Joseph P. and Sharp, Frank R.},
  year = {2012},
  month = may,
  journal = {Journal of Cerebral Blood Flow and Metabolism: Official Journal of the International Society of Cerebral Blood Flow and Metabolism},
  volume = {32},
  number = {5},
  pages = {780--791},
  issn = {1559-7016},
  doi = {10.1038/jcbfm.2011.179},
  abstract = {This study examined the effects of gender on RNA expression after ischemic stroke (IS). RNA obtained from blood of IS patients (n=51; 153 samples at {$<$} or =3, 5, and 24\,hours) and from matched controls (n=52) were processed on Affymetrix microarrays. Analyses of covariance for stroke versus control samples were performed separately for both genders and the regulated genes for females compared with males. In all, 242, 227, and 338 male-specific genes were regulated at {$<$} or =3, 5, and 24\,hours after IS, respectively, of which 59 were regulated at all time points. Overall, 774, 3,437, and 571 female-specific stroke genes were regulated at {$<$} or =3, 5, and 24\,hours, respectively, of which 152 were regulated at all time points. Male-specific stroke genes were associated with integrin, integrin-liked kinase, actin, tight junction, Wnt/{$\beta$}-catenin, RhoA, fibroblast growth factors (FGF), granzyme, and tumor necrosis factor receptor (TNFR)2 signaling. Female-specific stroke genes were associated with p53, high-mobility group box-1, hypoxia inducible factor (HIF)1{$\alpha$}, interleukin (IL)1, IL6, IL12, IL18, acute-phase response, T-helper, macrophage, and estrogen signaling. Cell death signaling was overrepresented in both genders, although the molecules and pathways differed. Gender affects gene expression in the blood of IS patients, which likely implies gender differences in immune, inflammatory, and cell death responses to stroke.},
  langid = {english},
  pmcid = {PMC3345909},
  pmid = {22167233},
  keywords = {Adolescent,Adult,Aged,Aged 80 and over,Brain Ischemia,Female,Gene Expression Profiling,Gene Expression Regulation,Humans,Male,Middle Aged,Oligonucleotide Array Sequence Analysis,Sex Characteristics,Stroke},
  file = {/Users/loverma2/Zotero/storage/8QSJPJD5/Tian e.a. - 2012 - Effects of gender on gene expression in the blood .pdf}
}

