@article{dinermanIncreasedNeutrophilElastase1990,
  title = {Increased Neutrophil Elastase Release in Unstable Angina Pectoris and Acute Myocardial Infarction},
  author = {Dinerman, J. L. and Mehta, J. L. and Saldeen, T. G. and Emerson, S. and Wallin, R. and Davda, R. and Davidson, A.},
