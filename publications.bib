  title = {Bayesian Networks in Healthcare: {{Distribution}} by Medical Condition},
  shorttitle = {Bayesian Networks in Healthcare},
  author = {McLachlan, Scott and Dube, Kudakwashe and Hitman, Graham A and Fenton, Norman E and Kyrimi, Evangelia},
  year = {2020},
