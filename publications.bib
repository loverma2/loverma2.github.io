  title = {Association of Systemic Inflammatory Markers with White Matter Hyperintensities and Microstructural Injury: An Analysis of {{UK Biobank}} Data},
  shorttitle = {Association of Systemic Inflammatory Markers with White Matter Hyperintensities and Microstructural Injury},
  author = {Qiao, Yuan and Zhao, Lei and Cong, Chaohua and Li, Yuna and Tian, Shan and Zhu, Xirui and Yang, Junting and Cao, Shanshan and Li, Panlong and Su, Jingjing},
  year = {2025},
