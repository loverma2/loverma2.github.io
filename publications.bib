@article{mielkeConsiderationSexGender2022,
  title = {Consideration of Sex and Gender in {{Alzheimer}}'s Disease and Related Disorders from a Global Perspective},
  author = {Mielke, Michelle M. and Aggarwal, Neelum T. and {Vila-Castelar}, Clara and Agarwal, Puja and {Arenaza-Urquijo}, Eider M. and Brett, Benjamin and {Brugulat-Serrat}, Anna and DuBose, Lyndsey E. and Eikelboom, Willem S. and Flatt, Jason and Foldi, Nancy S. and Franzen, Sanne and Gilsanz, Paola and Li, Wei and McManus, Alison J. and {van Lent}, Debora Melo and Milani, Sadaf Arefi and Shaaban, C. Elizabeth and Stites, Shana D. and Sundermann, Erin and Suryadevara, Vidyani and Trani, Jean-Francoise and Turner, Arlener D. and Vonk, Jet M. J. and Quiroz, Yakeel T. and Babulal, Ganesh M. and {Diversity and Disparity Professional Interest Area Sex and Gender Special Interest Group}},
  year = {2022},
  month = dec,
  journal = {Alzheimer's \& Dementia: The Journal of the Alzheimer's Association},
  volume = {18},
