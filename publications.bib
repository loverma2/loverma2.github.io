@article{haungsaithongChangesMeanPlatelet2015,
  title = {The {{Changes}} in {{Mean Platelet Volume}} after {{Using}} of {{Antiplatelet Drugs}} in {{Acute Ischemic Stroke}}: {{A Randomized Controlled Trial}}},
  shorttitle = {The {{Changes}} in {{Mean Platelet Volume}} after {{Using}} of {{Antiplatelet Drugs}} in {{Acute Ischemic Stroke}}},
  author = {Haungsaithong, Rojanant and Udommongkol, Chesda and Nidhinandana, Samart and Chairungsaris, Parnsiri and Chinvarun, Yotin and Suwantamee, Jithanorm and Sithinamsuwan, Pasiri},
  year = {2015},
