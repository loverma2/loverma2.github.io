  title = {Increased Leucocyte Count Could Predict Coronary Artery Calcification in Patients Free of Clinically Apparent Cardiovascular Disease},
  author = {Korkmaz, Levent and Kul, Selim and Korkmaz, Ay{\c c}a Ata and Aky{\"u}z, Ali R{\i}za and A{\u g}a{\c c}, Mustafa Tar{\i}k and Erkan, Hakan and Acar, Zeydin and Adar, Adem and Erku{\c s}, Muslihittin Emre and {\c C}elik, {\c S}{\"u}kr{\"u}},
  year = {2012},
