@article{jinHeterogeneousMultimodalBiomarkers2016,
  title = {Heterogeneous Multimodal Biomarkers Analysis for {{Alzheimer}}'s Disease via {{Bayesian}} Network},
  author = {Jin, Yan and Su, Yi and Zhou, Xiao-Hua and Huang, Shuai and {The Alzheimer's Disease Neuroimaging Initiative}},
