  pmid = {15780773},
  keywords = {Adult,Aged,Cause of Death,Coronary Disease,Female,Follow-Up Studies,Humans,Leukocyte Count,Lymphocyte Subsets,Male,Middle Aged,Risk Factors,United States}
}

