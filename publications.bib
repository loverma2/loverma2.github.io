@article{cekirdekciLevelAnxietyDepression2019,
  title = {Level of {{Anxiety}} and {{Depression}} in {{Cardiac Syndrome X}}},
  author = {Cekirdekci, Elif Ijlal and Bugan, Baris},
  year = {2019},
  journal = {Medical Principles and Practice: International Journal of the Kuwait University, Health Science Centre},
  volume = {28},
